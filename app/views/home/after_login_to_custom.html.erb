    <%= hidden_field_tag 'custom[gpu_price]', @params['gpu_price'] %>
    <%= hidden_field_tag 'custom[os]', @params['os'] %>
    <%= hidden_field_tag 'custom[os_url]', @params['os_url'] %>
    <%= hidden_field_tag 'custom[os_price]', @params['os_price'] %>
    <%= hidden_field_tag 'custom[memory]', @params['memory'] %>
    <%= hidden_field_tag 'custom[memory_url]', @params['memory_url'] %>
    <%= hidden_field_tag 'custom[memory_price]', @params['memory_price'] %>
    <%= hidden_field_tag 'custom[case]', @params['case'] %>
    <%= hidden_field_tag 'custom[case_url]', @params['case_url'] %>
    <%= hidden_field_tag 'custom[case_price]', @params['case_price'] %>
    <%= hidden_field_tag 'custom[m2]', @params['m2'] %>
    <%= hidden_field_tag 'custom[m2_url]', @params['m2_url'] %>
    <%= hidden_field_tag 'custom[m2_price]', @params['m2_price'] %>
    <%= hidden_field_tag 'custom[ssd]', @params['ssd'] %>
    <%= hidden_field_tag 'custom[ssd_url]', @params['ssd_url'] %>
    <%= hidden_field_tag 'custom[ssd_price]', @params['ssd_price'] %>
    <%= hidden_field_tag 'custom[hdd]', @params['hdd'] %>
    <%= hidden_field_tag 'custom[hdd_url]', @params['hdd_url'] %>
    <%= hidden_field_tag 'custom[hdd_price]', @params['hdd_price'] %>
    <%= hidden_field_tag 'custom[psu]', @params['psu'] %>
    <%= hidden_field_tag 'custom[psu_url]', @params['psu_url'] %>
