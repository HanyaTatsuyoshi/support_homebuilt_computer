    <%= hidden_field_tag 'custom[cooler_price]', @params['cooler_price'] %>
    <%= hidden_field_tag 'custom[peripheral]', @params['peripheral'] %>
    <%= hidden_field_tag 'custom[peripheral_url]', @params['peripheral_url'] %>
    <%= hidden_field_tag 'custom[peripheral_price]', @params['peripheral_price'] %>
    <% end %>
    <% end %>
    <noscript>
      <%=%>
      <%= button_tag '戻る', class: 'btn_sub'%>
    </noscript>
  </body>
</html>